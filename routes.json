[{
    "settings": ["master"],

    "index": {
        "verbs": ["get"],
        "path": "/",
        "call": "sellatweet.index"
    },

    "login": {
        "verbs": ["post"],
        "path": "/login",
        "call": "sellatweet.login"
    },

    "logout": {
        "verbs": ["get"],
        "path": "/logout",
        "call": "sellatweet.logout"
    },

    "create": {
        "verbs": ["post"],
        "path": "/create",
        "call": "sellatweet.create"
    },

    "tweets": {
        "verbs": ["get"],
        "path": "/tweets.json",
        "call": "sellatweet.tweets"
    }
}]